extern f1();
extern f2();
extern f3();
extern f4();

def A(x)
    if x > 0 then
        if x > 1 then
            f1()
        else
            f2()
    else
        if x > -1 then
            f3()
        else
            f4()
;

A(1.5);
A(0.5);
A(-0.5);
A(-1.5);

def B(x)
    if x > 0 & x < 1 then
        f1()
    else
        f2()
;

B(0.5);
B(1.5);
